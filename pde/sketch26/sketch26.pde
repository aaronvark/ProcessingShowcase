PFont f;
  
void setup() {
  size(720, 480);
  textSize(30);
  
  // Create the font
  f = createFont("arcadeclassic1.ttf", 24);
  textFont(f);
}


// berekent het aantal frames to een beat (obv BPM)
int bpmToFrameCount(int bpm)
{
  float bms = 60000/bpm;
  float fms = 1000/frameRate;
  return int(bms/fms);
}


void draw() {
  if ( frameCount % bpmToFrameCount(140) < 3)
  {
    eye2() ;
  } else if( frameCount % bpmToFrameCount(140) > 4) {
    eye3() ;
  } else {
    eye1() ;
  }
}


