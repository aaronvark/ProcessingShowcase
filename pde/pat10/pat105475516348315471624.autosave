void setup() {
size(720, 480);
noStroke();
fill(0);
frameRate(30);
}
void draw() {
background(0);
for (int x = 20; x < 400; x += 20) {
for (int y = 20; y < 400; y += 20) {
if ( random(0,10) > 5 ){
fill(0,0,random(255));
rect(x, y, 15, 15);
} else {
// draw a diamond using quad();
fill(0);
noStroke();
quad( x + 8, y + -2, x + 35, y + 8, x + 8, y + 35, x + -2 , y + 8 );
}
}
}
}
